    '''''''''''''''''''''''''''''Actuator Specifications'''''''''''''''''''''''''''''

    Private Sub bcalact_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bcalact.Click
        DataGridView1.Columns.Clear()

        Dim k As Decimal
        Dim pmax As Decimal

        pmax = 0

        For k = 0 To tbnline.Text - 1

            If pmax > max.Item(k).pmaxline Then
                pmax = pmax
            Else
                pmax = max.Item(k).pmaxline
            End If

        Next
        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        tbpmax.Text = pmax * 10 ^ 5

        DataGridView1.Columns.Add("Actuator No.", "Actuator No.")

        DataGridView1.Columns.Add("Pressure1 (Bar)", "Pressure1 (Bar)")
        DataGridView1.Columns.Add("Pressure2 (Bar)", "Pressure2 (Bar)")

        DataGridView1.Columns.Add("Flowrate1 (gpm(USA))", "Flowrate1 (gpm(USA))")
        DataGridView1.Columns.Add("Flowrate2 (gpm(USA))", "Flowrate2 (gpm(USA))")

        DataGridView1.Columns.Add("Velocity1 (m/sec) or (RPM)", "Velocity1 (m/sec) or (RPM)")
        DataGridView1.Columns.Add("Velocity2 (m/sec) or (RPM)", "Velocity2 (m/sec) or (RPM)")

        'DataGridView1.Columns.Add("Extending Time (sec)", "Extending Time (sec)")
        'DataGridView1.Columns.Add("Reacting Time (sec)", "Reacting Time (sec)")

        Dim j As Decimal

        j = tbnline.Text

        For j = 0 To tbnline.Text - 1

            If j = 0 Then
                For i = 0 To cyliser.Item(j).nser - 1
                    DataGridView1.Rows.Add((i + 1).ToString, line1.Item(i).pressure1.ToString, line1.Item(i).pressure2.ToString, line1.Item(i).flowrate1.ToString, line1.Item(i).flowrate2.ToString, line1.Item(i).Velocity1.ToString, line1.Item(i).Velocity2.ToString)
                Next

            ElseIf j = 1 Then
                For i = 0 To cyliser.Item(j).nser - 1
                    DataGridView1.Rows.Add((i + 1).ToString, line2.Item(i).pressure1.ToString, line2.Item(i).pressure2.ToString, line1.Item(i).flowrate1.ToString, line1.Item(i).flowrate2.ToString, line2.Item(i).Velocity1.ToString, line2.Item(i).Velocity2.ToString)
                Next

            ElseIf j = 2 Then
                For i = 0 To cyliser.Item(j).nser - 1
                    DataGridView1.Rows.Add((i + 1).ToString, line2.Item(i).pressure1.ToString, line2.Item(i).pressure2.ToString, line1.Item(i).flowrate1.ToString, line1.Item(i).flowrate2.ToString, line2.Item(i).Velocity1.ToString, line2.Item(i).Velocity2.ToString)
                Next

            End If

        Next



    End Sub

    Private Sub bprintcyl_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bprintact.Click
        If DataGridView1.Rows.Count = 0 Then
            MsgBox("The Table is Empty. Please Calculate The Results")
            Exit Sub
        End If
        Dim font As System.Drawing.Font
        Dim fontt As System.Drawing.Font

        'If dgv.Width > 1000 Then
        '    psd1.PageSettings.Landscape = True
        'Else
        '    psd1.PageSettings.Landscape = False
        'End If
        If psd1.ShowDialog = System.Windows.Forms.DialogResult.Cancel Then
            Exit Sub
        End If
        fontt = Label1.Font
        font = DataGridView1.Font
        p = New zaidprint_grid(pd1, DataGridView1, font, "spcification of cylinder", fontt)
        pvd1.ShowDialog()
        Dim ssss As MsgBoxResult
        ssss = MsgBox("Are you want to print", MsgBoxStyle.YesNo)
        If ssss = MsgBoxResult.No Then
            Exit Sub
        End If
        p = New zaidprint_grid(pd1, DataGridView1, font, Label1.Text, fontt)

        pd1.Print()
    End Sub

    Private Sub cbQpump_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles cbQpump.DropDown
        last = cbQpump.Text
    End Sub
    Private Sub cbQpump_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cbQpump.SelectedIndexChanged

        Try
            Select Case (last & cbQpump.Text)
                Case "gpm(USA)gpm(UK)"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) * 0.83267382).ToString
                Case "gpm(UK)gpm(USA)"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) / 0.83267382).ToString

                Case "gpm(USA)L/min"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) * 3.78541178).ToString
                Case "L/mingpm(USA)"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) / 3.78541178).ToString

                Case "gpm(USA)m3/sec"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) * 0.00006309).ToString
                Case "m3/secgpm(USA)"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) / 0.00006309).ToString

                Case "gpm(UK)L/min"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) * 4.546092).ToString
                Case "L/mingpm(UK)"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) / 4.546092).ToString
                Case "gpm(UK)m3/sec"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) * 0.00007577).ToString
                Case "m3/secgpm(UK)"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) / 0.00007577).ToString
                Case "L/minm3/sec"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) * 0.000016666667).ToString
                Case "m3/secL/min"
                    tbQpump.Text = (CType(tbQpump.Text, Decimal) / 0.000016666667).ToString
            End Select
        Catch ex As Exception
        End Try
    End Sub

    Private Sub bnline_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bnline.Click
        Dim cc As nlines

        If tbnline.Text = Int(tbnline.Text) Then
            For i = 0 To tbnline.Text - 1
                cc.nline = i
                cylilin.Add(cc)
                dser.ShowDialog()
            Next
        Else
            MsgBox("Please Input Real Number")
        End If

    End Sub

    '''''''''''''''''''''''''''''Tank Volume'''''''''''''''''''''''''''''

    Private Sub bcaltank_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bcaltank.Click
        Try
            Dim Qpump2 As Decimal = tbQpump2.Text

            '''''''''''''''''''''''''''''Convert'''''''''''''''''''''''''''''

            Qpump2 = convert_to_m3psec(Qpump2, cbQpump2.Text)

            '''''''''''''''''''''''''''''''''''''لتحويل الى دقيقة''''''''''''''''''''''''''''''

            Qpump2 = Qpump2 * 60

            Dim voil As Decimal
            Dim voil2 As Decimal
            Dim vtank As Decimal
            Dim vtank2 As Decimal

            Dim L As Decimal
            Dim L2 As Decimal
            Dim H As Decimal
            Dim H2 As Decimal
            Dim W As Decimal
            Dim W2 As Decimal

            Dim dsuction1 As Decimal
            Dim dsuction2 As Decimal
            Dim dreturn1 As Decimal
            Dim dreturn2 As Decimal
            Dim dpressure1 As Decimal
            Dim dpressure2 As Decimal
            Dim dsh As Decimal
            Dim dsh2 As Decimal

            If RadioButton27.Checked = Enabled Then

                voil = 3 * Qpump2
                vtank = 1.25 * voil

                voil2 = 5 * Qpump2
                vtank2 = 1.25 * voil2

            ElseIf RadioButton28.Checked = Enabled Then

                voil = 5 * Qpump2
                vtank = 1.25 * voil

                voil2 = 8 * Qpump2
                vtank2 = 1.25 * voil2

            End If

            W = (vtank) ^ (1 / 3)
            W2 = (vtank2) ^ (1 / 3)

            L = 1.5 * W
            L2 = 1.5 * W2

            H = 0.65 * W
            H2 = 0.65 * W2

            '''''''''''''''''''''''''''''''''''لارجاعها الى ثانية''''''''''''''''''''''''
            Qpump2 = Qpump2 / 60

            dsuction1 = ((4 / 3.14) * (Qpump2 / 1.6)) ^ 0.5
            dsuction2 = ((4 / 3.14) * (Qpump2 / 0.6)) ^ 0.5

            dreturn1 = ((4 / 3.14) * (Qpump2 / 3)) ^ 0.5
            dreturn2 = ((4 / 3.14) * (Qpump2 / 1.6)) ^ 0.5

            dpressure1 = ((4 / 3.14) * (Qpump2 / 4.5)) ^ 0.5
            dpressure2 = ((4 / 3.14) * (Qpump2 / 3)) ^ 0.5

            dsh = ((4 / 3.14) * (Qpump2 / 6)) ^ 0.5
            dsh2 = ((4 / 3.14) * (Qpump2 / 4.5)) ^ 0.5


            tbdsuc.Text = dsuction1
            tbdsuc2.Text = dsuction2

            tbdret.Text = dreturn1
            tbdret2.Text = dreturn2

            tbdpre.Text = dpressure1
            tbdpre2.Text = dpressure2

            tbdsh.Text = dsh
            tbdsh2.Text = dsh2


            tbL.Text = L
            tbLL.Text = L2

            tbH.Text = H
            tbHH.Text = H2

            tbW.Text = W
            tbWW.Text = W2

            tbvoil.Text = voil
            tbvoil2.Text = voil2

            tbvtank.Text = vtank
            tbvtank2.Text = vtank2
        Catch ex As Exception
            MsgBox("Please Check The Input Data, and Don't Input Letters or Symbols")
        End Try
    End Sub

    Private Sub cbQpump2_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles cbQpump2.DropDown
        last = cbQpump2.Text
    End Sub
    Private Sub cbQpump2_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cbQpump2.SelectedIndexChanged
        Try
            Select Case (last & cbQpump2.Text)
                Case "gpm(USA)gpm(UK)"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) * 0.83267382).ToString
                Case "gpm(UK)gpm(USA)"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) / 0.83267382).ToString
                Case "gpm(USA)L/min"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) * 3.78541178).ToString
                Case "L/mingpm(USA)"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) / 3.78541178).ToString
                Case "gpm(USA)m3/sec"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) * 0.00006309).ToString
                Case "m3/secgpm(USA)"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) / 0.00006309).ToString
                Case "gpm(UK)L/min"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) * 4.546092).ToString
                Case "L/mingpm(UK)"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) / 4.546092).ToString
                Case "gpm(UK)m3/sec"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) * 0.00007577).ToString
                Case "m3/secgpm(UK)"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) / 0.00007577).ToString
                Case "L/minm3/sec"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) * 0.000016666667).ToString
                Case "m3/secL/min"
                    tbQpump2.Text = (CType(tbQpump2.Text, Decimal) / 0.000016666667).ToString
            End Select
        Catch ex As Exception
            'MsgBox("Please Input Number Only")
        End Try
    End Sub

    Private Sub ComboBo_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles ComboBo.DropDown
        last = ComboBo.Text
    End Sub
    Private Sub ComboBo_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles ComboBo.SelectedIndexChanged
        Try
            Select Case (last & ComboBo.Text)
                Case "m(meter)cm(centimeter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) * 100).ToString
                Case "cm(centimeter)m(meter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) / 100).ToString
                Case "m(meter)mm(millimeter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) * 1000).ToString
                Case "mm(millimeter)m(meter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) / 1000).ToString
                Case "m(meter)ft(feet)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) * 3.28083989501312).ToString
                Case "ft(feet)m(meter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) / 3.28083989501312).ToString
                Case "m(meter)in(inch)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) * 39.3700787401575).ToString
                Case "in(inch)m(meter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) / 39.3700787401575).ToString
                Case "cm(centimeter)mm(millimeter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) * 10).ToString
                Case "mm(millimeter)cm(centimeter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) / 10).ToString
                Case "ft(feet)cm(centimeter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) * 30.48).ToString
                Case "cm(centimeter)ft(feet)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) / 30.48).ToString
                Case "in(inch)cm(centimeter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) * 2.54).ToString
                Case "cm(centimeter)in(inch)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) / 2.54).ToString
                Case "in(inch)mm(millimeter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) * 25.4).ToString
                Case "mm(millimeter)in(inch)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) / 25.4).ToString
                Case "ft(feet)in(inch)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) * 12).ToString
                Case "in(inch)ft(feet)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) / 12).ToString
                Case "ft(feet)mm(millimeter)"
                    tbdsuc.Text = (CType(tbdsuc.Text, Decimal) * 304.8).ToString
