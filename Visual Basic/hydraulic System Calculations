    '''''''''''''''''''''''''''''Power Losses'''''''''''''''''''''''''''''

    Private Sub bcallossp_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bcallossp.Click
        Try

            DataGridView3.Columns.Clear()
            'DataGridView7.Columns.Clear()

            cbep.SelectedIndex = 0
            cbmp.SelectedIndex = 0
            cbprv.SelectedIndex = 0

            Dim qa As powerlosses

            Dim asdv As Decimal
            Dim aswq As Decimal

            Dim pmax2 As Decimal

            Dim pmaxx As Decimal

            Dim x As Decimal

            Dim i As Integer

            Dim totalpdpump As Decimal
            Dim totalplpump As Decimal

            totalpdpump = 0
            totalplpump = 0

            'For i = 0 To tbnpump.Text - 1

            '    qa = pump.Item(i)

            '    asdv = qa.pdpump
            '    aswq = qa.plpump

            '    totalpdpump = totalpdpump + asdv
            '    totalplpump = totalplpump + aswq

            'Next

            Dim totalpdact As Decimal
            Dim totalplact As Decimal

            totalpdact = 0
            totalplact = 0

            For i = 0 To tbnact.Text - 1

                qa = actuator.Item(i)

                asdv = qa.pdact
                aswq = qa.plact

                x = qa.pmax

                If pmax2 >= x Then

                    pmaxx = pmax2

                ElseIf pmax2 < x Then

                    pmaxx = x

                End If

                pmax2 = x

                totalpdact = totalpdact + asdv
                totalplact = totalplact + aswq

            Next

            Dim totalpdpip As Decimal
            Dim totalplpip As Decimal

            totalpdpip = 0
            totalplpip = 0


            For i = 0 To tbnpipe.Text - 1

                qa = pipe.Item(i)

                asdv = qa.pdpipe
                aswq = qa.plpipe

                totalpdpip = totalpdpip + asdv
                totalplpip = totalplpip + aswq

            Next


            Dim totalpdfit As Decimal
            Dim totalplfit As Decimal

            totalpdfit = 0
            totalplfit = 0


            For i = 0 To tbnfitting.Text - 1

                qa = fitting.Item(i)

                asdv = qa.pdfitting
                aswq = qa.plfitting

                totalpdfit = totalpdfit + asdv
                totalplfit = totalplfit + aswq

            Next


            Dim pdsystem As Decimal
            Dim plsystem As Decimal

            pdsystem = totalpdact + totalpdfit + totalpdpip + totalpdpump
            plsystem = totalplact + totalplfit + totalplpip + totalplpump

            Dim prv As Decimal
            Dim pmotoro As Decimal
            Dim ppumpin As Decimal

            Dim psystem As Decimal
            Dim pw As Decimal
            Dim Qpump3 As Decimal = tbQpump3.Text

            Dim pelectrical As Decimal
            Dim amper As Decimal
            Dim volteg As Decimal


            Qpump3 = convert_to_m3psec(Qpump3, cbQpump3.Text)


            pw = pmaxx + pdsystem

            prv = 1.1 * pw
            prv = Math.Round(prv, 3)
            tbprv.Text = prv

            '''''''''''''''''''''القدرة المنظومة المشغلة فعلاً'''''''''''''''''''''''
            psystem = pw * Qpump3

            ''''''''''''''''''''هذه قدرة مضخة المنظومة اذا انفتح الرليف فالف ''''''''''''''''
            ppumpin = (prv * Qpump3) / 0.95

            ''''''''''''''''''''''''''''(1-0.95)* قدرة المضخة فعلا بدون الرليف فالف ''''''''''''''''''''''''
            plsystem = plsystem + 0.05 * (psystem / 0.95)

            pmotoro = ppumpin
            pmotoro = Math.Round(pmotoro, 3)
            tbpmotoro.Text = pmotoro

            amper = pmotoro / (0.9 * 220)
            amper = Math.Round(amper, 3)
            tbamper.Text = amper

            volteg = 220
            tbvolteg.Text = volteg

            pelectrical = amper * 220
            pelectrical = Math.Round(pelectrical, 3)
            tbpelectrical.Text = pelectrical


            pdsystem = Math.Round(pdsystem / 100000, 3)
            totalpdpump = Math.Round(totalpdpump / 100000, 3)
            totalpdact = Math.Round(totalpdact / 100000, 3)
            totalpdpip = Math.Round(totalpdpip / 100000, 3)
            totalpdfit = Math.Round(totalpdfit / 100000, 3)

            plsystem = Math.Round(plsystem / 1000, 3)
            totalplpump = Math.Round(totalplpump / 1000, 3)
            totalplact = Math.Round(totalplact / 1000, 3)
            totalplpip = Math.Round(totalplpip / 1000, 3)
            totalplfit = Math.Round(totalplfit / 1000, 3)

            DataGridView3.Columns.Add("", "")
            DataGridView3.Columns.Add("Hydraulic System", "Hydraulic System")
            'DataGridView2.Columns.Add("Hydraulic Pumps", "Hydraulic Pumps")
            DataGridView3.Columns.Add("Hydraulic Actuators", "Hydraulic Actuators")
            DataGridView3.Columns.Add("Hydraulic Conductors", "Hydraulic Conductors")
            DataGridView3.Columns.Add("Hydraulic Valves and Fittings", "Hydraulic Valves and Fittings")

            DataGridView3.Rows.Add("Pressure Drop (Bar)".ToString, pdsystem.ToString, totalpdact.ToString, totalpdpip.ToString, totalpdfit.ToString)
            DataGridView3.Rows.Add("Power Losses (kW)".ToString, plsystem.ToString, totalplact.ToString, totalplpip.ToString, totalplfit.ToString)

            'DataGridView7.Columns.Add("Power Losses of System (KW)", "Power Losses of System (KW)")
            'DataGridView7.Columns.Add("Power Losses of Pumps (KW)", "Power Losses of Pumps (KW)")
            'DataGridView7.Columns.Add("Power Losses of Actuators (KW)", "Power Losses of Actuators (KW)")
            'DataGridView7.Columns.Add("Power Losses of Pipes (KW)", "Power Losses of Pipes (KW)")
            'DataGridView7.Columns.Add("Power Losses of Fittings (KW)", "Power Losses of Fittings (KW)")


        Catch ex As Exception
            MsgBox("Please Check The Input Data, and Don't Input Letters or Symbols")
        End Try
    End Sub

    Private Sub bprintlosses_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bprintlosses.Click

        If DataGridView3.Rows.Count = 0 Then
            MsgBox("The Table is Empty. Please Calculate The Results")
            Exit Sub
        End If
        Dim font As System.Drawing.Font
        Dim fontt As System.Drawing.Font

        'If dgv.Width > 1000 Then
        '    psd1.PageSettings.Landscape = True
        'Else
        '    psd1.PageSettings.Landscape = False
        'End If
        If psd1.ShowDialog = System.Windows.Forms.DialogResult.Cancel Then
            Exit Sub
        End If
