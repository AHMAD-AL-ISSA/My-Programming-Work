    '''''''''''''''''''''''''''''Temperature Difference'''''''''''''''''''''''''''''

    Private Sub bcaltemp_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bcaltemp.Click
        Try
            DataGridView2.Columns.Clear()

            Dim den As Decimal = tbdfs.Text
            Dim cp As Decimal = tbcpfs.Text
            Dim vis As Decimal = tbvisfs.Text
            Dim Re As Decimal
            Dim dpp As Decimal
            Dim DaltaT As Decimal

            ''''''''''''''''''''''convert'''''''''''''''''''''''''''''''''
            den = convert_to_kgpm3(den, ComboBox8.Text)
            cp = convert_to_JpKgK(cp, ComboBox18.Text)
            vis = convert_to_m2psec(vis, ComboBox4.Text)



            '    Dim qmax As Decimal = TextBox24.Text
            '    Dim L As Decimal = TextBox22.Text
            '    Dim D As Decimal = TextBox21.Text
            '    Dim pmax As Decimal


            '    Re = 4 * qmax / (Math.PI * vis * D)

            '    If Re <= 2100 Then
            '        dpp = 128 * den * vis * L * qmax / (D ^ 4 * Math.PI)


            '    End If
            'If Re >= 4000 Then
            '    dpp = 0.1364 * ((4 / (vis * Math.PI)) ^ -0.25) * 16 * den / (2 * (Math.PI) ^ 2) * (qmax ^ 1.75) * (D ^ -4.75) * L

            'End If
            'For pmax = TextBox23.Text To TextBox20.Text Step TextBox19.Text

            '    'G = (1 - ep) * (1 / ep) + (1 - em) + dpp / pmax

            '    'G = (1 - ep) * (1 / ep) + (1 - em) + 0.2
            '    'G = dpp / pmax


            'DaltaT = dpp / (den * cp)


            'DataGridView5.Rows.Add(pmax, DaltaT)

            'Next
            'End If




            If RadioButton19.Checked = True Then

                DataGridView2.Columns.Add("Oil Flow Rate (m3/sec)", "Oil Flow Rate (m3/sec)")
                DataGridView2.Columns.Add("(Toil.out - Toil.in)", "(Toil.out - Toil.in)")

                Dim L As Decimal = TextBox22.Text
                Dim D As Decimal = TextBox21.Text
                Dim qmax As Decimal

                Dim qmax1 As Decimal = TextBox24.Text
                Dim qmax2 As Decimal = TextBox26.Text
                Dim qmax3 As Decimal = TextBox18.Text


                ''''''''''''''''''''''''''''''''''''''''''convert'''''''''''''''''''''''''''''''''''''''''''''''
                L = convert_to_meter(L, ComboBox32.Text)
                D = convert_to_meter(D, ComboBox33.Text)

                qmax1 = convert_to_m3psec(qmax1, cbQ.Text)
                qmax2 = convert_to_m3psec(qmax2, cbQ2.Text)
                qmax3 = convert_to_m3psec(qmax3, ComboBox37.Text)


                For qmax = qmax1 To qmax2 Step qmax3


                    Re = 4 * qmax / (Math.PI * vis * D)

                    If Re <= 2100 Then

                        dpp = 128 * den * vis * L * qmax / (D ^ 4 * Math.PI)

                    End If

                    If Re >= 2100 Then

                        dpp = 0.1364 * ((4 / (vis * Math.PI)) ^ -0.25) * 16 * den / (2 * (Math.PI) ^ 2) * (qmax ^ 1.75) * (D ^ -4.75) * L

                    End If


                    'G = (1 - ep) * (1 / ep) + (1 - em) + dpp / pmax
                    DaltaT = dpp / (den * cp)
                    DaltaT = Math.Round(DaltaT, 3)
                    DataGridView2.Rows.Add(qmax, DaltaT)

                Next

            End If

            If RadioButton20.Checked = True Then

                DataGridView2.Columns.Add("Tube Length (m)", "Tube Length (m)")
                DataGridView2.Columns.Add("(Toil.out - Toil.in)", "(Toil.out - Toil.in)")


                Dim qmax As Decimal = TextBox24.Text
                Dim D As Decimal = TextBox21.Text
                Dim L1 As Decimal = TextBox22.Text
                Dim L2 As Decimal = TextBox17.Text
                Dim L3 As Decimal = TextBox25.Text


                ''''''''''''''''''''''''''''''''''''''''''convert'''''''''''''''''''''''''''''''''''''''''''''''
                D = convert_to_meter(D, ComboBox33.Text)
                qmax = convert_to_m3psec(qmax, cbQ.Text)

                L1 = convert_to_meter(L1, ComboBox32.Text)
                L2 = convert_to_meter(L2, ComboBox11.Text)
                L3 = convert_to_meter(L3, ComboBox30.Text)


                For L = L1 To L2 Step L3


                    Re = 4 * qmax / (Math.PI * vis * D)

                    If Re <= 2100 Then
                        dpp = 128 * den * vis * L * qmax / (D ^ 4 * Math.PI)

                    End If
                    If Re >= 4000 Then
                        dpp = 0.1364 * ((4 / (vis * Math.PI)) ^ -0.25) * 16 * den / (2 * (Math.PI) ^ 2) * (qmax ^ 1.75) * (D ^ -4.75) * L

                    End If

                    'G = (1 - ep) * (1 / ep) + (1 - em) + dpp / pmax

                    DaltaT = dpp / (den * cp)
                    DaltaT = Math.Round(DaltaT, 3)
                    DataGridView2.Rows.Add(L, DaltaT)

                Next
            End If

            If RadioButton21.Checked = True Then

                DataGridView2.Columns.Add("Tube Diameter (mm)", "Tube Diameter (mm)")
                DataGridView2.Columns.Add("(Toil.out - Toil.in)", "(Toil.out - Toil.in)")

                Dim qmax As Decimal = TextBox24.Text
                Dim L As Decimal = TextBox22.Text
                Dim D1 As Decimal = TextBox21.Text
                Dim D2 As Decimal = TextBox32.Text
                Dim D3 As Decimal = TextBox31.Text


                ''''''''''''''''''''''''''''''''''''''''''convert'''''''''''''''''''''''''''''''''''''''''''''''
                L = convert_to_meter(L, ComboBox32.Text)
                qmax = convert_to_m3psec(qmax, cbQ.Text)

                D1 = convert_to_meter(D1, ComboBox33.Text)
                D2 = convert_to_meter(D2, ComboBox43.Text)
                D3 = convert_to_meter(D3, ComboBox44.Text)


                For D = D1 To D2 Step D3


                    Re = 4 * qmax / (Math.PI * vis * D)

                    If Re <= 2100 Then
                        dpp = 128 * den * vis * L * qmax / (D ^ 4 * Math.PI)

                    End If
                    If Re >= 4000 Then
                        dpp = 0.1364 * ((4 / (vis * Math.PI)) ^ -0.25) * 16 * den / (2 * (Math.PI) ^ 2) * (qmax ^ 1.75) * (D ^ -4.75) * L

                    End If

                    'G = (1 - ep) * (1 / ep) + (1 - em) + dpp / pmax


                    DaltaT = dpp / (den * cp)
                    DaltaT = Math.Round(DaltaT, 3)
                    DataGridView2.Rows.Add(Math.Round(D * 1000, 3), DaltaT)

                Next

            End If
        Catch ex As Exception
            MsgBox("Please Check The Input Data, and Don't Input Letters or Symbols")
        End Try
    End Sub

    Private Sub bprinttemp_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bprinttemp.Click
        If DataGridView2.Rows.Count = 0 Then
            MsgBox("The Table is Empty. Please Calculate The Results")
            Exit Sub
        End If
        Dim font As System.Drawing.Font
        Dim fontt As System.Drawing.Font

        'If dgv.Width > 1000 Then
        '    psd1.PageSettings.Landscape = True
        'Else
        '    psd1.PageSettings.Landscape = False
        'End If
        If psd1.ShowDialog = System.Windows.Forms.DialogResult.Cancel Then
            Exit Sub
        End If
        fontt = Label1.Font
        font = DataGridView2.Font
        p = New zaidprint_grid(pd1, DataGridView2, font, "Temperature Different", fontt)
        pvd1.ShowDialog()
        Dim ssss As MsgBoxResult
        ssss = MsgBox("Are you want to print", MsgBoxStyle.YesNo)
        If ssss = MsgBoxResult.No Then
            Exit Sub
        End If
        p = New zaidprint_grid(pd1, DataGridView2, font, "Temperature Different", fontt)

        pd1.Print()
    End Sub

    Private Sub bexceltemp_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bexceltemp.Click
        Try
            Dim wb As Excel.Workbook
            'Dim wb1 As Microsoft.Office.Interop.Excel.Workbook
            If DataGridView2.Rows.Count = 0 Then
                MsgBox("The Table is Empty. Please Calculate The Results")
                Exit Sub
            End If
            Dim diag As New System.Windows.Forms.FontDialog
            ' psd1.ShowDialog()
            If diag.ShowDialog = System.Windows.Forms.DialogResult.Cancel Then
                Exit Sub
            End If

            Dim ex As New Excel.Application
            Dim dialog As New SaveFileDialog
            Dim paths As String
            Dim result As DialogResult = dialog.ShowDialog
            Dim i As Integer
            Dim r As Integer
            If result = System.Windows.Forms.DialogResult.Cancel Then
                Exit Sub
            End If
            paths = dialog.FileName
            '/////////////////////////////////////////
            '  wb = ex.Workbooks.Add("C:\Documents and Settings\Administrator\Desktop\dmreportswork\dmreports\z.xls")
            wb = ex.Workbooks.Add(My.Computer.FileSystem.SpecialDirectories.ProgramFiles.ToString & "\HSC\Excel.xlsx")

            'wb = ex.Workbooks.Add(My.Computer.FileSystem.SpecialDirectories.Desktop & "\Excel.xlsx")
            'wb = ex.Workbooks.Add(My.Application.Info.DirectoryPath & "\z.xlsx")
            'Dim sheet As Excel.Worksheet = wb.Worksheets.Add
            Dim sheet As Excel.Worksheet = wb.Worksheets(1)
            sheet.Unprotect()

            '////////////////////////////////////////////////////////////////////
            'For i = 0 To DataGridView7.ColumnCount - 1
            For i = 0 To 1
                sheet.Cells(1, i + 1) = DataGridView2.Columns(i).HeaderText
                'sheet.Cells.Columns.AutoFit()
            Next
            sheet.Range(sheet.Cells(1, 1), sheet.Cells(1, i)).HorizontalAlignment = XlHAlign.xlHAlignCenter
            sheet.Range(sheet.Cells(1, 1), sheet.Cells(1, i)).HorizontalAlignment = XlHAlign.xlHAlignCenter

            '\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
            For r = 0 To DataGridView2.RowCount - 1

                'For i = 0 To DataGridView7.ColumnCount - 1
                For i = 0 To 1
                    sheet.Cells(r + 2, i + 1) = DataGridView2.Item(i, r).Value.ToString()
                Next i
            Next r

            sheet.Cells.BorderAround()
            sheet.Cells.Font.Size = diag.Font.Size
            sheet.Cells.Font.Name = diag.Font.Name
            sheet.Cells.Rows.AutoFit()
            sheet.Cells.Columns.AutoFit()
            sheet.Cells.VerticalAlignment = XlHAlign.xlHAlignCenter
            sheet.Cells.HorizontalAlignment = XlHAlign.xlHAlignLeft
            sheet.EnableOutlining = True

            If result = System.Windows.Forms.DialogResult.OK Then
                wb.SaveAs(dialog.FileName)
                ex.Visible = True
                wb.Activate()
            End If
        Catch ex As Exception
            MsgBox("The Table is Empty. Please Calculate The Results")
            MsgBox(ex.Message)
        End Try
    End Sub

