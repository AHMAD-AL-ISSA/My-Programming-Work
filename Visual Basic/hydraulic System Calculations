                    tbLL.Text = (CType(tbLL.Text, Decimal) * 100).ToString
                Case "cm(centimeter)m(meter)"
                    tbLL.Text = (CType(tbLL.Text, Decimal) / 100).ToString
                Case "m(meter)mm(millimeter)"
                    tbLL.Text = (CType(tbLL.Text, Decimal) * 1000).ToString
                Case "mm(millimeter)m(meter)"
                    tbLL.Text = (CType(tbLL.Text, Decimal) / 1000).ToString
                Case "m(meter)ft(feet)"
                    tbLL.Text = (CType(tbLL.Text, Decimal) * 3.28083989501312).ToString
                Case "ft(feet)m(meter)"
                    tbLL.Text = (CType(tbLL.Text, Decimal) / 3.28083989501312).ToString
                Case "m(meter)in(inch)"
                    tbLL.Text = (CType(tbLL.Text, Decimal) * 39.3700787401575).ToString
