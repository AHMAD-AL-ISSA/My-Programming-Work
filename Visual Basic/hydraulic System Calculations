    '''''''''''''''''''''''''''''Air Cooled'''''''''''''''''''''''''''''

    Private Sub bcalaircooled_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bcalaircooled.Click
        Try
            DataGridView6.Columns.Clear()

            Dim QL2 As Decimal = tbQL2.Text

            Dim Lt2 As Decimal = tbLt2.Text
            Dim Ht As Decimal = tbHt.Text
            Dim Dt As Decimal = tbDt.Text
            Dim Tt As Decimal = tbTt.Text

            Dim Pf As Decimal = tbPf.Text
            Dim Lf As Decimal = tbLf.Text
            Dim Df As Decimal = tbDf.Text
            Dim Tf As Decimal = tbTf.Text
            Dim kt2 As Decimal = tbkt2.Text

            Dim Ui2 As Decimal = tbUi2.Text
            Dim Uf2 As Decimal = tbUf2.Text

            Dim N As Decimal = tbN.Text

            Dim t33 As Decimal
            Dim t44 As Decimal

            Dim U2 As Decimal

            Dim Ati As Decimal
            Dim Ato As Decimal
            Dim Pti As Decimal
            Dim Pto As Decimal
            Dim Dhl As Decimal

            Dim R2 As Decimal
            Dim S2 As Decimal
            Dim F2 As Decimal
            Dim Tln2 As Decimal

            Dim L2 As Decimal
            Dim D2 As Decimal


            Dim NUl As Decimal

            Dim Nr As Decimal
            Dim Nt As Decimal

            Dim Rel As Decimal

            Dim ul As Decimal

            Dim cc As liquid
            Dim dd As air

            '''''''''''''''''''''''''''''
            Dim ml As Decimal

            Dim Prl As Decimal

            '''''''''''''''''''''''''''''

            Dim hl As Decimal

            'Dim A2 As Decimal
            Dim H2 As Decimal


            Dim Aa As Decimal
            Dim Pa As Decimal
            Dim Dha As Decimal

            Dim NUa As Decimal
            Dim Rea As Decimal

            Dim Pra As Decimal

            Dim ha As Decimal
            Dim Ga As Decimal

            Dim tma As Decimal
            Dim ma As Decimal

            Dim vl As Decimal
            Dim Til As Decimal
            Dim Tol As Decimal
            Dim dl As Decimal
            Dim RDl As Decimal
            Dim Kl As Decimal
            Dim visl As Decimal
            Dim cpl As Decimal

            '''''''''''''''''''''''''''''

            Dim Tia As Decimal
            Dim Toa As Decimal
            Dim RDa As Decimal
            Dim Ka As Decimal
            Dim da As Decimal
            Dim visa As Decimal
            Dim cpa As Decimal

            '''''''''''''''''''''''''''''

            cc = Liquidd.Item(0)

            dl = cc.denliquid
            Til = cc.temliquidi
            cpl = cc.specificheatliquid
            Kl = cc.kliquid
            visl = cc.visliquid
            RDl = cc.RDliquid

            ml = cc.massliquid
            vl = cc.flowliquid
            Prl = cc.prandtlliquid
            '''''''''''''''''''''''''''''

            dd = Airr.Item(0)

            da = dd.denair
            Tia = dd.temairi
            Toa = dd.temairo
            cpa = dd.specificheatair
            Ka = dd.kair
            visa = dd.visair
            RDa = dd.RDair

            Pra = dd.prandtlair


            '''''''''''''''''''''''''''''convert'''''''''''''''''''''''''''''
            QL2 = convert_to_w(QL2, cbQL2.Text)

            'Lt2 = convert_to_meter(Lt2, cbLt2.Text)
            Ht = convert_to_meter(Ht, cbHt.Text)
            Dt = convert_to_meter(Dt, cbDt.Text)
            Tt = convert_to_meter(Tt, cbTt.Text)
            kt2 = convert_to_WpmK(kt2, cbkt2.Text)

            Pf = convert_to_meter(Pf, cbPf.Text)
            Lf = convert_to_meter(Lf, cbLf.Text)
            Df = convert_to_meter(Df, cbDf.Text)
            Tf = convert_to_meter(Tf, cbTf.Text)

            Ui2 = convert_to_Wpm2K(Ui2, cbUi2.Text)
            Uf2 = convert_to_Wpm2K(Uf2, cbUf2.Text)


            '''''''''''''''''''''''''''''calculation'''''''''''''''''''''''''''''

            Dim XT As Decimal = Ht + 2 * Tt + 0.006
            Dim XL As Decimal = Dt + Ht + 0.003


            Tol = Til - QL2 / (ml * cpl)

            tbTol.Text = Tol

            tma = QL2 / (cpa * (Toa - Tia))

            tbtma.Text = tma / 1.06

            ma = tma / N


            t33 = Til - Toa

            t44 = Tol - Tia

            Dim azaz As Decimal

            azaz = Math.Log(t33 / t44)


            Tln2 = (t33 - t44) / azaz

            R2 = (Til - Tol) / (Toa - Tia)
            S2 = (Toa - Tia) / (Til - Tia)

            F2 = ((Math.Sqrt(R2 ^ 2 + 1)) / (R2 - 1)) * Math.Log(((1 - S2) / (1 - R2 * S2))) / Math.Log(((2 - S2 * (R2 + 1 - (Math.Sqrt(R2 ^ 2 + 1)))) / ((2 - S2 * (R2 + 1 + (Math.Sqrt(R2 ^ 2 + 1)))))))



            DataGridView6.Columns.Add("", "")
            DataGridView6.Columns.Add("Heat Load (kW)", "Heat Load (kW)")
            DataGridView6.Columns.Add("Heat Transfer Area (m2)", "Heat Transfer Area (m2)")
            DataGridView6.Columns.Add("Number of Tubes", "Number of Tubes")
            DataGridView6.Columns.Add("Face Velocity of Air (m/sec)", "Face Velocity of Air (m/sec)")
            DataGridView6.Columns.Add("Height of H. E. (m)", "Height of H. E. (m)")
            DataGridView6.Columns.Add("Length of H. E. (m)", "Length of H. E. (m)")
            DataGridView6.Columns.Add("Width of H. E. (m)", "Width of H. E. (m)")
            DataGridView6.Columns.Add("Assumed Overall Coefficient (W/m2.K)", "Assumed Overall Coefficient (W/m2.K)")
            DataGridView6.Columns.Add("Design Overall Coefficient (W/m2.K)", "Design Overall Coefficient (W/m2.K)")


            For Nr = 3 To 8

                For U2 = Uf2 To Ui2 Step -1

                    Dim Act As Decimal

                    Act = (Ht - 2 * Tt) * (Dt - Ht) + (Math.PI * 0.25 * (Ht - 2 * Tt) ^ 2)

                    'Ato = Ht * (Dt - Ht) + (Math.PI * 0.25 * Ht ^ 2)

                    Pti = 2 * (Dt - Ht) + Math.PI * (Ht - 2 * Tt)

                    Ati = Pti * Lt2

                    Pto = 2 * (Dt - Ht) + Math.PI * (Ht)

                    Ato = Pto * Lt2


                    Nt = QL2 / (U2 * Ato * F2 * Tln2)


                    Nt = Nr * Math.Ceiling(Nt / Nr)

                    Dim AAA As Decimal
                    AAA = Math.Round(Nt * Ato, 3)

                    L2 = XT * ((Nt / Nr) + 1)

                    D2 = (XL * (Nr - 1)) + Dt
                    'ul = vl / (Nr * Nt * Al)
                    H2 = Lt2

                    Dim FV As Decimal
                    FV = Math.Round((tma / 1.06) / (H2 * L2), 3)

                    If FV >= 4.5 Then
                        GoTo line400
                    End If

                    '''''''''''''''''''''''''''''liquid'''''''''''''''''''''''''''''
                    ul = vl / ((Nt / Nr) * Act)

                    Dhl = (4 * Act) / Pti

                    Rel = (ul * Dhl) / visl
                    '''''''''''''''''''''''''''''
                    NUl = 0.023 * Rel ^ 0.8 * Prl ^ 0.4
                    '''''''''''''''''''''''''''''Heat Transfer Coefficients'''''''''''''''''''''''''''''
                    hl = Math.Round(NUl * (Kl / Dhl), 3)




                    '''''''''''''''''''''''''''''air'''''''''''''''''''''''''''''
                    Aa = (Pf / 2) * (XT - Ht)

                    Pa = Pf + 2 * Lf

                    Dha = (4 * Aa) / Pa


                    Ga = ma / Aa

                    Rea = (Dha * Ga) / (visa / da)

                    '''''''''''''''''''''''''''''laminar'''''''''''''''''''''''''''''
                    If Rea <= 2100 Then
                        NUa = 1.86 * ((Rea * Pra * Dha) / (Df)) ^ (1 / 3)
                        '''''''''''''''''''''''''''''turbulent'''''''''''''''''''''''''''''
                    ElseIf Rea >= 10 ^ 4 Then
                        NUa = 0.023 * Rea ^ 0.8 * Pra ^ 0.3
                        '''''''''''''''''''''''''''''transmition'''''''''''''''''''''''''''''
                    End If
                    '''''''''''''''''''''''''''''Heat Transfer Coefficients'''''''''''''''''''''''''''''
                    ha = Math.Round(NUa * (Ka / Dha), 3)

                    Dim Uf22 As Decimal

                    Uf22 = 1 / (((1 / ha) + ((Ato * Math.Log(Ato / Ati)) / (2 * kt2)) + Ato / (hl * Ati)) + ((RDl * Ato) / Ati) + RDa)

                    Uf22 = Math.Round(Uf22, 3)

                    If 20 > (Uf22 - U2) And (Uf22 - U2) > 0 Then

                        If Nr = 3 Then

                            DataGridView6.Rows.Add("No. of Rows=3", QL2 / 1000, AAA, Nt, FV, H2, L2, D2, U2, Uf22)

                        ElseIf Nr = 4 Then

                            DataGridView6.Rows.Add("No. of Rows=4", QL2 / 1000, AAA, Nt, FV, H2, L2, D2, U2, Uf22)

                        ElseIf Nr = 5 Then

                            DataGridView6.Rows.Add("No. of Rows=5", QL2 / 1000, AAA, Nt, FV, H2, L2, D2, U2, Uf22)

                        ElseIf Nr = 6 Then

                            DataGridView6.Rows.Add("No. of Rows=6", QL2 / 1000, AAA, Nt, FV, H2, L2, D2, U2, Uf22)

                        ElseIf Nr = 7 Then

                            DataGridView6.Rows.Add("No. of Rows=7", QL2 / 1000, AAA, Nt, FV, H2, L2, D2, U2, Uf22)

                        ElseIf Nr = 8 Then

                            DataGridView6.Rows.Add("No. of Rows=8", QL2 / 1000, AAA, Nt, FV, H2, L2, D2, U2, Uf22)

                        End If

                    Else

                        GoTo line400

                    End If

                    GoTo line500
line400:

                Next
line500:

            Next

        Catch ex As Exception
            MsgBox("Please Check The Input Data, and Don't Input Letters or Symbols")
        End Try
    End Sub

    Private Sub printaircooled_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles printaircooled.Click
        If DataGridView6.Rows.Count = 0 Then
            MsgBox("The Table is Empty. Please Calculate The Results")
            Exit Sub
        End If
        Dim font As System.Drawing.Font
        Dim fontt As System.Drawing.Font

        'If dgv.Width > 1000 Then
        '    psd1.PageSettings.Landscape = True
        'Else
        '    psd1.PageSettings.Landscape = False
        'End If
        If psd1.ShowDialog = System.Windows.Forms.DialogResult.Cancel Then
            Exit Sub
        End If
        fontt = Label1.Font
        font = DataGridView6.Font
        p = New zaidprint_grid(pd1, DataGridView6, font, "Design Heat Exchanger", fontt)
        pvd1.ShowDialog()
        Dim ssss As MsgBoxResult
        ssss = MsgBox("Are you want to print", MsgBoxStyle.YesNo)
        If ssss = MsgBoxResult.No Then
            Exit Sub
        End If
        p = New zaidprint_grid(pd1, DataGridView6, font, "Design Heat Exchanger", fontt)

        pd1.Print()
    End Sub

