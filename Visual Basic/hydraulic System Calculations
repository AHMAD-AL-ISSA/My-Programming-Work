                Case "mm(millimeter)m(meter)"
                    TextBox21.Text = (CType(TextBox21.Text, Decimal) / 1000).ToString
                Case "m(meter)ft(feet)"
                    TextBox21.Text = (CType(TextBox21.Text, Decimal) * 3.28083989501312).ToString
                Case "ft(feet)m(meter)"
                    TextBox21.Text = (CType(TextBox21.Text, Decimal) / 3.28083989501312).ToString
                Case "m(meter)in(inch)"
                    TextBox21.Text = (CType(TextBox21.Text, Decimal) * 39.3700787401575).ToString
                Case "in(inch)m(meter)"
                    TextBox21.Text = (CType(TextBox21.Text, Decimal) / 39.3700787401575).ToString
                Case "cm(centimeter)mm(millimeter)"
