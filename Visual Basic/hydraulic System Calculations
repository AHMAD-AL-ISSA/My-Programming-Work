    Private Sub Button10_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button10.Click

        If RadioButton1.Checked = Enabled Then
            dHydraulic_System.ShowDialog()
        ElseIf RadioButton2.Checked = Enabled Then
            dHydraulic_Reservoir.ShowDialog()
        ElseIf RadioButton3.Checked = Enabled Then
            dHydraulic_Pump.ShowDialog()
        ElseIf RadioButton4.Checked = Enabled Then
            dHydraulic_Motor.ShowDialog()
        End If

    End Sub







    Private Sub RadioButton5_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles RadioButton5.CheckedChanged

        If RadioButton5.Checked = True Then
            RadioButton22.Enabled = True
            RadioButton29.Enabled = True
            RadioButton30.Enabled = True
            RadioButton31.Enabled = True

            RadioButton33.Enabled = False
            RadioButton34.Enabled = False

            RadioButton6.Enabled = False
            RadioButton14.Enabled = False

        End If

    End Sub



    Private Sub RadioButton23_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles RadioButton23.CheckedChanged
        If RadioButton23.Checked = True Then
            RadioButton33.Enabled = True
            RadioButton34.Enabled = True

            RadioButton22.Enabled = False
            RadioButton29.Enabled = False
            RadioButton30.Enabled = False
            RadioButton31.Enabled = False

            RadioButton6.Enabled = False
            RadioButton14.Enabled = False

        End If
    End Sub

    Private Sub RadioButton32_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles RadioButton32.CheckedChanged
        If RadioButton32.Checked = True Then
      
            RadioButton33.Enabled = False
            RadioButton34.Enabled = False

            RadioButton22.Enabled = False
            RadioButton29.Enabled = False
            RadioButton30.Enabled = False
            RadioButton31.Enabled = False

            RadioButton6.Enabled = True
            RadioButton14.Enabled = True
        End If
    End Sub

    Private Sub Button11_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button11.Click
        If RadioButton22.Checked = True Then
            RichTextBox1.Show()
            RichTextBox2.Hide()
            RichTextBox3.Hide()
            RichTextBox4.Hide()
            RichTextBox5.Hide()

        ElseIf RadioButton29.Checked = True Then
            RichTextBox1.Hide()
            RichTextBox2.Show()
            RichTextBox3.Hide()
            RichTextBox4.Hide()
            RichTextBox5.Hide()

        ElseIf RadioButton30.Checked = True Then
            RichTextBox1.Hide()
            RichTextBox2.Hide()
            RichTextBox3.Show()
            RichTextBox4.Hide()
            RichTextBox5.Hide()

        ElseIf RadioButton31.Checked = True Then
            RichTextBox1.Hide()
            RichTextBox2.Hide()
            RichTextBox3.Hide()
            RichTextBox4.Show()
            RichTextBox5.Hide()

        ElseIf RadioButton33.Checked = True Then
            RichTextBox1.Hide()
            RichTextBox2.Hide()
            RichTextBox3.Show()
            RichTextBox4.Hide()
            RichTextBox5.Hide()


        ElseIf RadioButton34.Checked = True Then
            RichTextBox1.Hide()
            RichTextBox2.Show()
            RichTextBox3.Hide()
            RichTextBox4.Hide()
            RichTextBox5.Hide()

        ElseIf RadioButton6.Checked = True Then
            RichTextBox1.Hide()
            RichTextBox2.Hide()
            RichTextBox3.Hide()
            RichTextBox4.Hide()
            RichTextBox5.Show()

        Else : RadioButton14.Checked = True
            RichTextBox1.Hide()
            RichTextBox2.Hide()
            RichTextBox3.Hide()
            RichTextBox4.Show()
            RichTextBox5.Hide()

        End If
    End Sub

