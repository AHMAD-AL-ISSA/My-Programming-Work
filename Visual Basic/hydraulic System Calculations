    Private Sub pd1_PrintPage(ByVal sender As System.Object, ByVal e As System.Drawing.Printing.PrintPageEventArgs) Handles pd1.PrintPage
        Dim b As Boolean = p.Print(e.Graphics)
        If b = True Then
            e.HasMorePages = True
        End If
    End Sub

