        font = DataGridView3.Font
        p = New zaidprint_grid(pd1, DataGridView3, font, "Power Losses", fontt)
        pvd1.ShowDialog()
        Dim ssss As MsgBoxResult
        ssss = MsgBox("Are you want to print", MsgBoxStyle.YesNo)
        If ssss = MsgBoxResult.No Then
            Exit Sub
        End If
        p = New zaidprint_grid(pd1, DataGridView3, font, "Power Losses", fontt)

        pd1.Print()
    End Sub

