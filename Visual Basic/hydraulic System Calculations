    Private Sub cbQL2_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles cbQL2.DropDown
        last = cbQL2.Text
    End Sub
    Private Sub cbQL2_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cbQL2.SelectedIndexChanged
        Try
            Select Case (last & cbQL2.Text)
                Case "WkW"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) / 1000).ToString
                Case "kWW"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) * 1000).ToString
                Case "WHP"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) * 0.001341022).ToString
                Case "HPW"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) / 0.001341022).ToString
                Case "WIb.ft/sec"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) * 0.737562149).ToString
                Case "Ib.ft/secW"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) / 0.737562149).ToString
                Case "kWHP"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) * 1.34102209).ToString
                Case "HPkW"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) / 1.34102209).ToString
                Case "kWIb.ft/sec"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) * 737.562149277).ToString
                Case "Ib.ft/seckW"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) / 737.562149277).ToString
                Case "HPIb.ft/sec"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) * 550.0).ToString
                Case "Ib.ft/secHP"
                    tbQL2.Text = (CType(tbQL2.Text, Decimal) / 550.0).ToString
            End Select
        Catch ex As Exception
            'MsgBox("Please Input Number Only")
        End Try
    End Sub

