    '''''''''''''''''''''''''''''''''''''''''''''''''''''Thermal Condition'''''''''''''''''''''''''''''

    Private Sub bcalcheck_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bcalcheck.Click
        Try

            Dim aa As hydraulicoil
            Dim bb As hydraulicsys

            aa = oilproper.Item(0)



            Dim doil As Decimal
            Dim visoil As Decimal
            Dim cpoil As Decimal
            Dim Boil As Decimal
            Dim Koil As Decimal
            Dim aoil As Decimal
            Dim Tm As Decimal

            doil = aa.denoil
            visoil = aa.viscosityoil
            cpoil = aa.specificheatoil
            Boil = aa.Blukoil
            Koil = aa.thermaloil
            aoil = aa.cubickoil
            Tm = aa.Tmax

            DataGridView4.Columns.Clear()







            Dim ton As Decimal = tbton.Text
            Dim Toil As Decimal

            '''''''''''''''''''''''''''''convert'''''''''''''''''''''''''''''




            ton = convert_to_sec(ton, cbton.Text)



            '''''''''''''''''''''''''''''hydraulic system'''''''''''''''''''''''''''''

            DataGridView4.Columns.Add("Time (sec)", "Time (sec)")
            DataGridView4.Columns.Add("Temperature of Oil Inside the Reservoir (C)", "Temperature of Oil Inside the Reservoir (C)")
            DataGridView4.Columns.Add("Is the Temperature of Oil within Allowed Range", "Is the Temperature of Oil within Allowed Range")

            For i = 0 To ton
                bb = hydraulicsystem.Item(i)
                Toil = bb.Tempoil

                If Toil > Tm Then

                    DataGridView4.Rows.Add(i, Toil, "No")

                Else

                    DataGridView4.Rows.Add(i, Toil, "Yes")

                End If

            Next











        Catch ex As Exception
            MsgBox("Please Check The Input Data, and Don't Input Letters or Symbols")
        End Try
    End Sub

    Private Sub bprintcheck_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bprintcheck.Click

        If DataGridView4.Rows.Count = 0 Then
            MsgBox("The Table is Empty. Please Calculate The Results")
            Exit Sub
        End If
        Dim font1 As System.Drawing.Font
        Dim font2 As System.Drawing.Font


        'If dgv.Width > 1000 Then
        '    psd1.PageSettings.Landscape = True
        'Else
        '    psd1.PageSettings.Landscape = False
        'End If
        If psd1.ShowDialog = System.Windows.Forms.DialogResult.Cancel Then
            Exit Sub
        End If

        font1 = DataGridView4.Font
        font2 = bahsc.Font

        p = New zaidprint_grid(pd1, DataGridView4, font1, "Thermal Condition", font1)
        pvd1.ShowDialog()
        Dim ssss As MsgBoxResult
        ssss = MsgBox("Are you want to print", MsgBoxStyle.YesNo)
        If ssss = MsgBoxResult.No Then
            Exit Sub
        End If
        p = New zaidprint_grid(pd1, DataGridView4, font1, "Thermal Condition", font1)

        pd1.Print()

    End Sub

    Private Sub bexcelcheck_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bexcelcheck.Click
        Try
            Dim wb As Excel.Workbook
            'Dim wb1 As Microsoft.Office.Interop.Excel.Workbook
            If DataGridView4.Rows.Count = 0 Then
                MsgBox("The Table is Empty. Please Calculate The Results")
                Exit Sub
            End If
            Dim diag As New System.Windows.Forms.FontDialog
            ' psd1.ShowDialog()
            If diag.ShowDialog = System.Windows.Forms.DialogResult.Cancel Then
                Exit Sub
            End If

            Dim ex As New Excel.Application
            Dim dialog As New SaveFileDialog
            Dim paths As String
            Dim result As DialogResult = dialog.ShowDialog
            Dim i As Integer
            Dim r As Integer
            If result = System.Windows.Forms.DialogResult.Cancel Then
                Exit Sub
            End If
            paths = dialog.FileName
            '/////////////////////////////////////////
            '  wb = ex.Workbooks.Add("C:\Documents and Settings\Administrator\Desktop\dmreportswork\dmreports\z.xls")
            wb = ex.Workbooks.Add(My.Computer.FileSystem.SpecialDirectories.ProgramFiles.ToString & "\HSC\Excel.xlsx")
            'wb = ex.Workbooks.Add(My.Application.Info.DirectoryPath & "\z.xlsx")
            'Dim sheet As Excel.Worksheet = wb.Worksheets.Add
            Dim sheet As Excel.Worksheet = wb.Worksheets(1)
            sheet.Unprotect()

            '////////////////////////////////////////////////////////////////////
            'For i = 0 To DataGridView7.ColumnCount - 1
            For i = 0 To 1
                sheet.Cells(1, i + 1) = DataGridView4.Columns(i).HeaderText
                'sheet.Cells.Columns.AutoFit()
            Next
            sheet.Range(sheet.Cells(1, 1), sheet.Cells(1, i)).HorizontalAlignment = XlHAlign.xlHAlignCenter
            sheet.Range(sheet.Cells(1, 1), sheet.Cells(1, i)).HorizontalAlignment = XlHAlign.xlHAlignCenter

            '\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
            For r = 0 To DataGridView4.RowCount - 2

                'For i = 0 To DataGridView7.ColumnCount - 1
                For i = 0 To 1
                    sheet.Cells(r + 2, i + 1) = DataGridView4.Item(i, r).Value.ToString()
                Next i
            Next r

            sheet.Cells.BorderAround()
            sheet.Cells.Font.Size = diag.Font.Size
            sheet.Cells.Font.Name = diag.Font.Name
            sheet.Cells.Rows.AutoFit()
            sheet.Cells.Columns.AutoFit()
            sheet.Cells.VerticalAlignment = XlHAlign.xlHAlignCenter
            sheet.Cells.HorizontalAlignment = XlHAlign.xlHAlignLeft
            sheet.EnableOutlining = True

            If result = System.Windows.Forms.DialogResult.OK Then
                wb.SaveAs(dialog.FileName)
                ex.Visible = True
                wb.Activate()
            End If
        Catch ex As Exception
            MsgBox("The Table is Empty. Please Calculate The Results")
            MsgBox(ex.Message)
        End Try
    End Sub



