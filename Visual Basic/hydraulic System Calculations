    Private Sub ComboBox15_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles ComboBox15.DropDown
        last = ComboBox15.Text
    End Sub
    Private Sub ComboBox15_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles ComboBox15.SelectedIndexChanged
        Try
            Select Case (last & ComboBox15.Text)
                Case "KC"
                    tbTi.Text = (CType(tbTi.Text, Decimal) - 273.15).ToString
                Case "CK"
                    tbTi.Text = (CType(tbTi.Text, Decimal) + 273.15).ToString
                Case "KF"
                    tbTi.Text = (CType(tbTi.Text, Decimal) * (9 / 5) - 459.67).ToString
                Case "FK"
                    tbTi.Text = ((CType(tbTi.Text, Decimal) + 459.67) * (5 / 9)).ToString
                Case "FC"
                    tbTi.Text = (((CType(tbTi.Text, Decimal) + 459.67) * (5 / 9)) - 273.15).ToString
                Case "CF"
                    tbTi.Text = ((CType(tbTi.Text, Decimal) + 273.15) * (9 / 5) - 459.67).ToString
            End Select
        Catch ex As Exception
            'MsgBox("Please Input Number Only")
        End Try
    End Sub

    Private Sub ComboBox16_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles ComboBox16.DropDown
        last = ComboBox16.Text
    End Sub
    Private Sub ComboBox16_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles ComboBox16.SelectedIndexChanged
        Try
