                Case "KF"
                    tbTam.Text = (CType(tbTam.Text, Decimal) * (9 / 5) - 459.67).ToString
                Case "FK"
                    tbTam.Text = ((CType(tbTam.Text, Decimal) + 459.67) * (5 / 9)).ToString
                Case "FC"
                    tbTam.Text = (((CType(tbTam.Text, Decimal) + 459.67) * (5 / 9)) - 273.15).ToString
                Case "CF"
                    tbTam.Text = ((CType(tbTam.Text, Decimal) + 273.15) * (9 / 5) - 459.67).ToString
            End Select
        Catch ex As Exception
            'MsgBox("Please Input Number Only")
        End Try
    End Sub

    

    



    Private Sub cbton_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles cbton.DropDown
