    Private Sub cbTol_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles cbTol.DropDown
        last = cbTol.Text
    End Sub
    Private Sub cbTol_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cbTol.SelectedIndexChanged
        Try
            Select Case (last & cbTol.Text)
                Case "KC"
                    tbTol.Text = (CType(tbTol.Text, Decimal) - 273.15).ToString
                Case "CK"
                    tbTol.Text = (CType(tbTol.Text, Decimal) + 273.15).ToString
                Case "KF"
                    tbTol.Text = (CType(tbTol.Text, Decimal) * (9 / 5) - 459.67).ToString
                Case "FK"
                    tbTol.Text = ((CType(tbTol.Text, Decimal) + 459.67) * (5 / 9)).ToString
                Case "FC"
                    tbTol.Text = (((CType(tbTol.Text, Decimal) + 459.67) * (5 / 9)) - 273.15).ToString
                Case "CF"
                    tbTol.Text = ((CType(tbTol.Text, Decimal) + 273.15) * (9 / 5) - 459.67).ToString
            End Select
        Catch ex As Exception
            'MsgBox("Please Input Number Only")
        End Try
    End Sub

    Private Sub cbtma_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles cbtma.DropDown
        last = cbtma.Text
    End Sub
    Private Sub cbtma_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cbtma.SelectedIndexChanged
        Try
            Select Case (last & cbtma.Text)
                Case "gpm(USA)gpm(UK)"
                    tbtma.Text = (CType(tbtma.Text, Decimal) * 0.83267382).ToString
                Case "gpm(UK)gpm(USA)"
                    tbtma.Text = (CType(tbtma.Text, Decimal) / 0.83267382).ToString
                Case "gpm(USA)L/min"
                    tbtma.Text = (CType(tbtma.Text, Decimal) * 3.78541178).ToString
                Case "L/mingpm(USA)"
                    tbtma.Text = (CType(tbtma.Text, Decimal) / 3.78541178).ToString
                Case "gpm(USA)m3/sec"
                    tbtma.Text = (CType(tbtma.Text, Decimal) * 0.00006309).ToString
                Case "m3/secgpm(USA)"
                    tbtma.Text = (CType(tbtma.Text, Decimal) / 0.00006309).ToString
                Case "gpm(UK)L/min"
                    tbtma.Text = (CType(tbtma.Text, Decimal) * 4.546092).ToString
                Case "L/mingpm(UK)"
                    tbtma.Text = (CType(tbtma.Text, Decimal) / 4.546092).ToString
                Case "gpm(UK)m3/sec"
                    tbtma.Text = (CType(tbtma.Text, Decimal) * 0.00007577).ToString
                Case "m3/secgpm(UK)"
                    tbtma.Text = (CType(tbtma.Text, Decimal) / 0.00007577).ToString
                Case "L/minm3/sec"
                    tbtma.Text = (CType(tbtma.Text, Decimal) * 0.000016666667).ToString
                Case "m3/secL/min"
                    tbtma.Text = (CType(tbtma.Text, Decimal) / 0.000016666667).ToString
            End Select
        Catch ex As Exception
            'MsgBox("Please Input Number Only")
        End Try
    End Sub

