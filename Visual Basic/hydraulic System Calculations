    End Sub
    Private Sub cbUi_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cbUi.SelectedIndexChanged
        Try
            Select Case (last & cbUi.Text)
                Case "kW/(m2.K)W/(m2.K)"
                    tbUi.Text = (CType(tbUi.Text, Decimal) * 1000).ToString
                Case "W/(m2.K)kW/(m2.K)"
                    tbUi.Text = (CType(tbUi.Text, Decimal) / 1000).ToString
                Case "kW/(m2.K)Btu/(h.ft2.F)"
                    tbUi.Text = (CType(tbUi.Text, Decimal) * 176.12).ToString
                Case "Btu/(h.ft2.F)kW/(m2.K)"
                    tbUi.Text = (CType(tbUi.Text, Decimal) / 176.12).ToString
                Case "W/(m2.K)Btu/(h.ft2.F)"
                    tbUi.Text = (CType(tbUi.Text, Decimal) * 0.17612).ToString
                Case "Btu/(h.ft2.F)W/(m2.K)"
                    tbUi.Text = (CType(tbUi.Text, Decimal) / 0.17612).ToString
            End Select
        Catch ex As Exception
            'MsgBox("Please Input Number Only")
        End Try
    End Sub

    Private Sub cbQL_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles cbQL.DropDown
        last = cbQL.Text
    End Sub
    Private Sub cbQL_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cbQL.SelectedIndexChanged
        Try
            Select Case (last & cbQL.Text)
                Case "WkW"
