    Private Sub cbton_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cbton.SelectedIndexChanged
        Try
            Select Case (last & cbton.Text)
                Case "hrmin"
                    tbton.Text = (CType(tbton.Text, Decimal) * 60).ToString
                Case "minhr"
                    tbton.Text = (CType(tbton.Text, Decimal) / 60).ToString
                Case "hrsec"
                    tbton.Text = (CType(tbton.Text, Decimal) * 3600).ToString
                Case "sechr"
                    tbton.Text = (CType(tbton.Text, Decimal) / 3600).ToString
                Case "minsec"
                    tbton.Text = (CType(tbton.Text, Decimal) * 60).ToString
                Case "secmin"
                    tbton.Text = (CType(tbton.Text, Decimal) / 60).ToString
            End Select
        Catch ex As Exception
            'MsgBox("Please Input Number Only")
        End Try
    End Sub

   
    Private Sub ComboBox51_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles ComboBox51.DropDown, ComboBox7.DropDown
        last = ComboBox51.Text
    End Sub
    Private Sub ComboBox51_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles ComboBox51.SelectedIndexChanged, ComboBox7.SelectedIndexChanged
        Try
            Select Case (last & ComboBox51.Text)
                Case "kW/(m2.K)W/(m2.K)"
                    tbhh2.Text = (CType(tbhh2.Text, Decimal) * 1000).ToString
                Case "W/(m2.K)kW/(m2.K)"
                    tbhh2.Text = (CType(tbhh2.Text, Decimal) / 1000).ToString
                Case "kW/(m2.K)Btu/(h.ft2.F)"
                    tbhh2.Text = (CType(tbhh2.Text, Decimal) * 176.12).ToString
                Case "Btu/(h.ft2.F)kW/(m2.K)"
                    tbhh2.Text = (CType(tbhh2.Text, Decimal) / 176.12).ToString
                Case "W/(m2.K)Btu/(h.ft2.F)"
                    tbhh2.Text = (CType(tbhh2.Text, Decimal) * 0.17612).ToString
                Case "Btu/(h.ft2.F)W/(m2.K)"
                    tbhh2.Text = (CType(tbhh2.Text, Decimal) / 0.17612).ToString
            End Select
        Catch ex As Exception
            'MsgBox("Please Input Number Only")
        End Try
    End Sub

   

    '''''''''''''''''''''''''''''Shell and Tube'''''''''''''''''''''''''''''

    Private Sub bcalshelltube_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bcalshelltube.Click
        Try
            DataGridView5.Columns.Clear()

            Dim aa As fluidtube
            Dim bb As fluidshell

            Dim QL As Decimal = tbQL.Text
            Dim dit As Decimal = tbdit.Text
            Dim dot As Decimal = tbdot.Text
            Dim lt As Decimal = tblt.Text
            Dim Kt As Decimal = tbK.Text
            Dim Ui As Decimal = tbUi.Text
            Dim Uf As Decimal = tbUf.Text

            Dim dft As Decimal
            Dim Tit As Decimal
            Dim Tot As Decimal
            Dim cpft As Decimal
            Dim Kft As Decimal
            Dim visft As Decimal
            Dim RDt As Decimal
            '''''''''''''''''''''''''''''
            Dim Prt As Decimal
            '''''''''''''''''''''''''''''
            Dim mt As Decimal
            Dim mtt As Decimal
            Dim Ret As Decimal
            Dim NUt As Decimal
            Dim ht As Decimal
            '''''''''''''''''''''''''''''
            Dim dfs As Decimal
            Dim Tis As Decimal
            Dim Tos As Decimal
            Dim cpfs As Decimal
            Dim Kfs As Decimal
            Dim visfs As Decimal
            Dim RDs As Decimal
            '''''''''''''''''''''''''''''
            Dim ms As Decimal
            Dim Prs As Decimal
            '''''''''''''''''''''''''''''
            Dim Res As Decimal
            Dim hs As Decimal
            Dim G As Decimal
            Dim a As Decimal
            Dim Tlns As Decimal
            Dim t11 As Decimal
            Dim t22 As Decimal
            Dim np As Decimal
            Dim jH As Decimal
            Dim R As Decimal
            Dim S As Decimal
            Dim F As Decimal
            Dim Db As Decimal
            Dim K1 As Decimal
            Dim n1 As Decimal
            Dim PT As Decimal
            Dim Ds As Decimal
            Dim CC As Decimal
            Dim B As Decimal
            Dim nt As Decimal
            Dim de As Decimal
            Dim y As Decimal

            '''''''''''''''''''''''''''''
            aa = ftube.Item(0)

            dft = aa.dtube
            Tit = aa.temfti
            Tot = aa.temfto
            cpft = aa.specifictube
            Kft = aa.ktube
            visft = aa.vistube
            RDt = aa.RDtube

            mt = aa.masst
            Prt = aa.prandtlt

            '''''''''''''''''''''''''''''
            bb = fshell.Item(0)

            dfs = bb.denshell
            Tis = bb.temfsi
            Tos = bb.temfso
            cpfs = bb.specificshell
            Kfs = bb.kshell
            visfs = bb.visshell
            RDs = bb.RDshell

            ms = bb.masss
            Prs = bb.prandtls



            '''''''''''''''''''''''''''''convert'''''''''''''''''''''''''''''
            QL = convert_to_w(QL, cbQL.Text)
            Ui = convert_to_Wpm2K(Ui, cbUi.Text)
            Uf = convert_to_Wpm2K(Uf, cbUf.Text)
            dit = convert_to_meter(dit, cbdit.Text)
            dot = convert_to_meter(dot, cbdot.Text)
            lt = convert_to_meter(lt, cblt.Text)
            Kt = convert_to_WpmK(Kt, cbK.Text)

            '''''''''''''''''''''''''''''calculation'''''''''''''''''''''''''''''

            DataGridView5.Columns.Add("Heat Load", "Heat Load ")
            DataGridView5.Columns.Add("U: W/(m2.K)", "U: W/(m2.K)")
            'DataGridView7.Columns.Add("uf: W/(m2.K)", "uf: W/(m2.K)")
            DataGridView5.Columns.Add("number of tubes pass", "number of tubes pass")
            DataGridView5.Columns.Add("number of tubes", "number of tubes ")
            DataGridView5.Columns.Add("mass flow rate tube", "mass flow rate tube ")
            DataGridView5.Columns.Add("Shell Diameter:m", "Shell Diameter:m")
            DataGridView5.Columns.Add("Bundle Diameter:m", "Bundle Diameter:m")

            'DataGridView7.Columns.Add("ht: W/(m2.K)", "ht: W/(m2.K)")
            'DataGridView7.Columns.Add("NUt", "NUt")
            'DataGridView7.Columns.Add("Ret", "Ret")
            'DataGridView7.Columns.Add("Prt", "Prt")
            'DataGridView7.Columns.Add("hs: W/(m2.K)", "hs: W/(m2.K)")
            'DataGridView7.Columns.Add("jH", "jH")
            'DataGridView7.Columns.Add("Res", "Res")
            'DataGridView7.Columns.Add("Prs", "Prs")



            Tos = Tis - QL / (ms * cpfs)

            tbTos.Text = Tos

            mtt = QL / (cpft * (Tot - Tit))

            tbvft.Text = mtt / dft

            PT = 1.25 * dot

            If RadioButton9.Checked = True Then
                de = (1.1 / dot) * ((PT) ^ 2 - 0.917 * (dot) ^ 2)

            ElseIf RadioButton10.Checked = True Then
                de = (1.27 / dot) * ((PT) ^ 2 - 0.785 * (dot) ^ 2)

            End If



            '''''''''''''''''''''''''''''

            'If RadioButton1.Checked = True Then
            t11 = Tis - Tot
            t22 = Tos - Tit

            'Else
            '    t11 = Tis - Tit
            '    t22 = Tos - Tot
            'End If


            Tlns = (t11 - t22) / Math.Log(t11 / t22)



            '''''''''''''''''''''''''''''any number of shell passes and any even number of tube passes'''''''''''''''''''''''''''''
            'R = (Tis - Tos) / (Tot - Tit)
            'PP = (Tot - Tit) / (Tis - Tit)

            'If R = 1 Then
            '    S = PP / (N - (N - 1) * PP)
            '    F = S * Math.Sqrt(2) / ((1 - S) * Math.Log((2 - S * (2 - Math.Sqrt(2))) / (2 - S * (2 + Math.Sqrt(2)))))
            'Else
            '    qq = ((1 - R * PP) / (1 - R)) ^ (1 / N)
            '    S = (qq - 1) / (qq - R)
            '    F = ((Math.Sqrt(R ^ 2 + 1)) / (R - 1)) * Math.Log(((1 - S) / (1 - R * S)) / ((2 - S * (R + 1 - (Math.Sqrt(R ^ 2 + 1)))) / ((2 - S * (R + 1 + (Math.Sqrt(R ^ 2 + 1)))))))

            'End If


            '''''''''''''''''''''''''''''one shell passes and any even number of tube passes'''''''''''''''''''''''''''''
            R = (Tis - Tos) / (Tot - Tit)
            S = (Tot - Tit) / (Tis - Tit)

            F = ((Math.Sqrt(R ^ 2 + 1)) / (R - 1)) * Math.Log(((1 - S) / (1 - R * S))) / Math.Log(((2 - S * (R + 1 - (Math.Sqrt(R ^ 2 + 1)))) / ((2 - S * (R + 1 + (Math.Sqrt(R ^ 2 + 1)))))))







            '''''''''''''''''''''''''''''يفضل ان تبدا من الكبير الى الصغير لات عدد الانابيب سوف يكون من الاقل الى الاكثر'''''''''''''''''''''''''''''

            For U = Uf To Ui Step -1


                nt = QL / (U * F * Tlns * Math.PI * dot * lt)

                nt = Math.Ceiling(nt)

                If (nt / 2) = Int(nt / 2) Then
                    GoTo line200
                Else
                    nt = nt + 1
                    GoTo line200
                End If

Line100:
                ''''''''''''''''''''''''''''' ft for np = 2,4,6,8 لا يمكن استخدامها لان '''''''''''''''''''''''''''''
                np = 1
                K1 = 0.319
                n1 = 2.142



                mt = mtt / (nt / np)
                Ret = (4 * mt) / (Math.PI * dit * visft * dft)

                Db = dot * (nt / K1) ^ (1 / n1)
                Db = 1000 * Db
                If RadioButton50.Checked = True Then
                    y = 0.01 * Db + 8
                    Ds = Math.Round((y + Db) / 1000, 5)
                ElseIf RadioButton602.Checked = True Then
                    y = 38
                    Ds = Math.Round((y + Db) / 1000, 5)
                ElseIf RadioButton7.Checked = True Then
                    y = 0.028 * Db + 44.4
                    Ds = Math.Round((y + Db) / 1000, 5)
                ElseIf RadioButton8.Checked = True Then
                    y = 0.009 * Db + 86.2
                    Ds = Math.Round((y + Db) / 1000, 5)
                End If

                CC = (PT - dot) / PT
                B = Ds / 5
                a = Ds * CC * B
                G = ms / a
                Res = (de * G) / (visfs * dfs)


                '''''''''''''''''''''''''''''Fluid of tube'''''''''''''''''''''''''''''
                '''''''''''''''''''''''''''''laminar'''''''''''''''''''''''''''''
                If Ret <= 2100 Then
                    NUt = 1.86 * ((Ret * Prt * dit) / (lt)) ^ (1 / 3)
                    '''''''''''''''''''''''''''''turbulent'''''''''''''''''''''''''''''
                ElseIf Ret >= 10 ^ 4 Then
                    NUt = 0.023 * Ret ^ 0.8 * Prt ^ 0.3
                    '''''''''''''''''''''''''''''transmition'''''''''''''''''''''''''''''
                Else
                    NUt = 0.116 * ((Ret) ^ (2 / 3) - 125) * Prt ^ (1 / 3) * (1 + (dit / lt) ^ 2 / 3)
                End If
                '''''''''''''''''''''''''''''Heat Transfer Coefficients'''''''''''''''''''''''''''''
                ht = Math.Round((Kft * NUt) / dit, 3)




                '''''''''''''''''''''''''''''Fluid of shell'''''''''''''''''''''''''''''

                jH = 0.5 * (1 + (B / Ds)) * (0.08 * (Res ^ 0.6821) + 0.7 * (Res ^ 0.1772))

                '''''''''''''''''''''''''''''Heat Transfer Coefficients'''''''''''''''''''''''''''''

                hs = Math.Round(jH * (Kfs / de) * Prs ^ (1 / 3), 3)


                '''''''''''''''''''''''''''''

                Uf = 1 / ((dot / (ht * dit)) + ((dot * Math.Log(dot / dit)) / (2 * Kt)) + (1 / hs) + ((RDs * dot) / dit) + RDt)

                '''''''''''''''''''''''''''''
                If (Uf > U) Then


                    'DataGridView7.Rows.Add(QL, U, uf, np, nt, Ds, ht, NUt, Ret, Prt, hs, jH, Res, Prs)
                    DataGridView5.Rows.Add(QL, U, np, nt, mt, Ds, Db)

                Else
                    GoTo line200

                End If

line200:

                '''''''''''''''''''''''''''''output'''''''''''''''''''''''''''''

                For np = 2 To 8 Step 2

                    If np = 2 Then
                        K1 = 0.249
                        n1 = 2.207
                    ElseIf np = 4 Then
                        K1 = 0.175
                        n1 = 2.285
                    ElseIf np = 6 Then
                        K1 = 0.0743
                        n1 = 2.499
                    Else : np = 8
                        K1 = 0.0365
                        n1 = 2.675
                    End If


                    mt = mtt / (nt / np)

                    Ret = (4 * mt) / (Math.PI * dit * visft * dft)

                    mt = Math.Round(mt, 3)

                    Db = dot * (nt / K1) ^ (1 / n1)

                    Db = 1000 * Db

                    If RadioButton50.Checked Or RadioButton16.Checked = True Then
                        y = 0.01 * Db + 8
                        Ds = Math.Round((y + Db) / 1000, 5)
                    ElseIf RadioButton602.Checked = True Then
                        y = 38
                        Ds = Math.Round((y + Db) / 1000, 5)
                    ElseIf RadioButton7.Checked = True Then
                        y = 0.028 * Db + 44.4
                        Ds = Math.Round((y + Db) / 1000, 5)
                    ElseIf RadioButton8.Checked = True Then
                        y = 0.009 * Db + 86.2
                        Ds = Math.Round((y + Db) / 1000, 5)
                    End If



                    Db = Math.Round(Db / 1000, 4)

                    CC = (PT - dot) / PT
                    B = Ds / 5
                    a = Ds * CC * B
                    G = ms / a
                    Res = (de * G) / (visfs * dfs)

                    '''''''''''''''''''''''''''''Fluid of tube'''''''''''''''''''''''''''''
                    '''''''''''''''''''''''''''''laminar'''''''''''''''''''''''''''''
                    If Ret <= 2100 Then
                        NUt = 1.86 * ((Ret * Prt * dit) / (lt)) ^ (1 / 3)
                        '''''''''''''''''''''''''''''turbulent'''''''''''''''''''''''''''''
                    ElseIf Ret >= 10 ^ 4 Then
                        NUt = 0.023 * Ret ^ 0.8 * Prt ^ 0.3
                        '''''''''''''''''''''''''''''transmition'''''''''''''''''''''''''''''
                    Else
                        NUt = 0.116 * ((Ret) ^ (2 / 3) - 125) * Prt ^ (1 / 3) * (1 + (dit / lt) ^ 2 / 3)
                    End If
                    '''''''''''''''''''''''''''''Heat Transfer Coefficients'''''''''''''''''''''''''''''
                    ht = Math.Round((Kft * NUt) / dit, 3)




                    '''''''''''''''''''''''''''''Fluid of shell'''''''''''''''''''''''''''''

                    jH = 0.5 * (1 + (B / Ds)) * (0.08 * (Res ^ 0.6821) + 0.7 * (Res ^ 0.1772))

                    '''''''''''''''''''''''''''''Heat Transfer Coefficients'''''''''''''''''''''''''''''

                    hs = Math.Round(jH * (Kfs / de) * Prs ^ (1 / 3), 3)

                    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

                    Uf = ((dot / (ht * dit)) + ((dot * Math.Log(dot / dit)) / (2 * Kt)) + (1 / hs) + ((RDt * dot) / dit) + RDs) ^ -1

                    ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
                    If (Uf > U) Then

                        'DataGridView7.Rows.Add(QL, U, uf, np, nt, Ds, ht, NUt, Ret, Prt, hs, jH, Res, Prs)
                        DataGridView5.Rows.Add(QL, U, np, nt, mt, Ds, Db)

                    Else
                        GoTo line300

                    End If

line300:

                    '''''''''''''''''''''''''''''output'''''''''''''''''''''''''''''

                Next
            Next

            MsgBox("Design has been")

        Catch ex As Exception
            MsgBox("Please Check The Input Data, and Don't Input Letters or Symbols")
        End Try
    End Sub

    Private Sub bprintshell_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles bprintshell.Click
        If DataGridView5.Rows.Count = 0 Then
            MsgBox("The Table is Empty. Please Calculate The Results")
            Exit Sub
        End If
        Dim font As System.Drawing.Font
        Dim fontt As System.Drawing.Font

        'If dgv.Width > 1000 Then
        '    psd1.PageSettings.Landscape = True
        'Else
        '    psd1.PageSettings.Landscape = False
        'End If
        If psd1.ShowDialog = System.Windows.Forms.DialogResult.Cancel Then
            Exit Sub
        End If
        fontt = Label1.Font
        font = DataGridView5.Font
        p = New zaidprint_grid(pd1, DataGridView5, font, "Design Heat Exchanger", fontt)
        pvd1.ShowDialog()
        Dim ssss As MsgBoxResult
        ssss = MsgBox("Are you want to print", MsgBoxStyle.YesNo)
        If ssss = MsgBoxResult.No Then
            Exit Sub
        End If
        p = New zaidprint_grid(pd1, DataGridView5, font, "Design Heat Exchanger", fontt)

        pd1.Print()
    End Sub

