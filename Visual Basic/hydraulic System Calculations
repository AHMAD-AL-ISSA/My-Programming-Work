    Private Sub cbTt_DropDown(ByVal sender As Object, ByVal e As System.EventArgs) Handles cbTt.DropDown
        last = cbTt.Text
    End Sub
    Private Sub cbTt_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cbTt.SelectedIndexChanged
        Try
            Select Case (last & cbTt.Text)
                Case "m(meter)cm(centimeter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) * 100).ToString
                Case "cm(centimeter)m(meter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) / 100).ToString
                Case "m(meter)mm(millimeter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) * 1000).ToString
                Case "mm(millimeter)m(meter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) / 1000).ToString
                Case "m(meter)ft(feet)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) * 3.28083989501312).ToString
                Case "ft(feet)m(meter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) / 3.28083989501312).ToString
                Case "m(meter)in(inch)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) * 39.3700787401575).ToString
                Case "in(inch)m(meter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) / 39.3700787401575).ToString
                Case "cm(centimeter)mm(millimeter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) * 10).ToString
                Case "mm(millimeter)cm(centimeter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) / 10).ToString
                Case "ft(feet)cm(centimeter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) * 30.48).ToString
                Case "cm(centimeter)ft(feet)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) / 30.48).ToString
                Case "in(inch)cm(centimeter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) * 2.54).ToString
                Case "cm(centimeter)in(inch)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) / 2.54).ToString
                Case "in(inch)mm(millimeter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) * 25.4).ToString
                Case "mm(millimeter)in(inch)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) / 25.4).ToString
                Case "ft(feet)in(inch)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) * 12).ToString
                Case "in(inch)ft(feet)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) / 12).ToString
                Case "ft(feet)mm(millimeter)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) * 304.8).ToString
                Case "mm(millimeter)ft(feet)"
                    tbTt.Text = (CType(tbTt.Text, Decimal) / 304.8).ToString
            End Select
        Catch ex As Exception
            'MsgBox("Please Input Number Only")
        End Try
    End Sub

